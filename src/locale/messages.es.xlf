      <trans-unit id="aboutus2" datatype="html">
        <source>SolarSPELL is a Solar Powered Educational Learning Library. Our initiative works to build internet-ready skills and improve educational opportunities around the world.</source>
        <target>SolarSPELL es una biblioteca de aprendizaje educativo con energía solar. Nuestra iniciativa trabaja para desarrollar habilidades listas para Internet y mejorar las oportunidades educativas en todo el mundo.</target>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/about/about.component.html</context>
          <context context-type="linenumber">17</context>
        </context-group>
        <note priority="1" from="description">aboutus2</note>
      </trans-unit>
      <trans-unit id="values1" datatype="html">
        <source>Our Values</source>
        <target>Nuestros Valores</target>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/about/about.component.html</context>
          <context context-type="linenumber">18</context>
        </context-group>
